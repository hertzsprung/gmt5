#-------------------------------------------------------------------------------
# Dependencies of data on CVS-produced originals

update: 	res_f/GSHHS_f_polygons.b res_h/GSHHS_h_polygons.b res_i/GSHHS_i_polygons.b \
		res_l/GSHHS_l_polygons.b res_c/GSHHS_c_polygons.b wdb/wdb_f_borders.b wdb/wdb_f_rivers.b

# All files used in coast/res_? are copies of the latest in the GSHHS+WDBII directories

res_f/GSHHS_f_polygons.b: GSHHS+WDBII/GSHHS/res_f/GSHHS_f_polygons.b
		cp -f GSHHS+WDBII/GSHHS/res_f/GSHHS_f_polygons.b res_f/GSHHS_f_polygons.b

res_h/GSHHS_h_polygons.b: GSHHS+WDBII/GSHHS/res_h/GSHHS_h_polygons.b
		cp -f GSHHS+WDBII/GSHHS/res_h/GSHHS_h_polygons.b res_h/GSHHS_h_polygons.b

res_i/GSHHS_i_polygons.b: GSHHS+WDBII/GSHHS/res_i/GSHHS_i_polygons.b
		cp -f GSHHS+WDBII/GSHHS/res_i/GSHHS_i_polygons.b res_i/GSHHS_i_polygons.b

res_l/GSHHS_l_polygons.b: GSHHS+WDBII/GSHHS/res_l/GSHHS_l_polygons.b
		cp -f GSHHS+WDBII/GSHHS/res_l/GSHHS_l_polygons.b res_l/GSHHS_l_polygons.b

res_c/GSHHS_c_polygons.b: GSHHS+WDBII/GSHHS/res_c/GSHHS_c_polygons.b
		cp -f GSHHS+WDBII/GSHHS/res_c/GSHHS_c_polygons.b res_c/GSHHS_c_polygons.b

wdb/wdb_f_borders.b:	GSHHS+WDBII/WDBII/borders/WDBII_Borders_segments.b
		cp -f GSHHS+WDBII/WDBII/borders/WDBII_Borders_segments.b wdb/wdb_f_borders.b

wdb/wdb_f_rivers.b:	GSHHS+WDBII/WDBII/rivers/WDBII_Rivers_segments.b
		cp -f GSHHS+WDBII/WDBII/rivers/WDBII_Rivers_segments.b wdb/wdb_f_rivers.b

#-------------------------------------------------------------------------------
# full Dependencies:
#-------------------------------------------------------------------------------
full:				res_f/binned_GSHHS_f.cdf res_f/binned_border_f.cdf res_f/binned_river_f.cdf
info_f:				res_f/GSHHS_f_info.lis

res_f/binned_GSHHS_f.cdf: 	res_f/binned_GSHHS_f.pt
				shoremaker res_f/binned_GSHHS_f
					
res_f/binned_GSHHS_f.pt:	res_f/GSHHS_f_nodes.nc
				polygon_to_bins res_f/GSHHS_f_polygons.b $(BIN_f) res_f/GSHHS_f_nodes.nc res_f/GSHHS_f_ndid.b res_f/binned_GSHHS_f

res_f/GSHHS_f_nodes.nc: 	res_f/GSHHS_f_polygons.b
				polygon_setnodes res_f/GSHHS_f_polygons.b $(BIN_f) res_f/GSHHS_f_nodes.nc res_f/GSHHS_f_ndid.b

res_f/GSHHS_f_info.lis: 	res_f/GSHHS_f_polygons.b
				polygon_final_info res_f/GSHHS_f_polygons.b > res_f/GSHHS_f_info.lis

res_f/binned_border_f.cdf:	res_f/binned_border_f.pt
				linemaker res_f/binned_border_f

res_f/binned_border_f.pt:	wdb/wdb_f_borders.b
				lines_to_bins wdb/wdb_f_borders.b $(BIN_f) res_f/binned_border_f

res_f/binned_river_f.cdf:	res_f/binned_river_f.pt
				linemaker res_f/binned_river_f

res_f/binned_river_f.pt:	wdb/wdb_f_rivers.b
				lines_to_bins wdb/wdb_f_rivers.b $(BIN_f) res_f/binned_river_f
#-------------------------------------------------------------------------------
